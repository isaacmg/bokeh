{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noEmitOnError": false,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES6",
    "lib": ["es6"],
    "types": ["node"],
    "baseUrl": "."
  },
  "include": ["./**/*.ts"]
}
